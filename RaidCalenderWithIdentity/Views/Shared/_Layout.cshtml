<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Raid Kalender</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    @* jquery *@
    <script src="~/Scripts/jquery-3.2.1.js"></script>
    @* angular *@

    <script src="~/Scripts/angular.min.js"></script>
    @* Calender *@
    <link href='~/Scripts/fullcalender-3.3.1/fullcalendar.min.css' rel='stylesheet' />
    <link href='~/Scripts/fullcalender-3.3.1/fullcalendar.print.min.css' rel='stylesheet' media='print' />
    <script src='~/Scripts/fullcalender-3.3.1/lib/moment.min.js'></script>
    @*<script src='~/Scripts/fullcalender-3.3.1/lib/jquery.min.js'></script>*@
    <script src='~/Scripts/fullcalender-3.3.1/fullcalendar.min.js'></script>
    <script src='~/Scripts/fullcalender-3.3.1/locale-all.js'></script>
    @* Calender *@
    
    @* custom *@
    <link href="~/Content/custom/window.css" rel="stylesheet" />
    <script src="~/Scripts/custom/calender.js"></script>
    @* custom *@
    @* Wichtig, da sonst das got net mvc framework nicht richtig angezeigt wird *@
    <link href="~/Content/Site.css" rel="stylesheet" />
    @* Timepicker *@
    <script src="~/Scripts/timepickerByjonthornton/jquery.timepicker.min.js"></script>
    <link href="~/Scripts/timepickerByjonthornton/jquery.timepicker.css" rel="stylesheet" />
</head>
<body ng-app="event" >

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Raid Calender", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("HoT Event Timer", "Index", "EventTimer")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Meine Guild Wars 2 Web Apps</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
