
@{
    ViewBag.Title = "_detailEvent";
}
<div id="detailEvent" class="modal" @*ng-controller="DetailEventController"*@>
    <!-- Modal content -->
    <div class="modal-content">

        <div class="modal-header">
            <span class="close">&times;</span>
            <h2>Modal Header</h2>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-2">
                    <label for="typ">Typ:</label>
                    <span name="typ" id="typ"></span>
                </div>
                <div class="col-md-2">
                    <label for="name">Bezeichnung:</label>
                    <span name="name" id="name"></span>
                    <div id="detail_raidImg"></div>
                </div>
                <div class="col-md-2">
                    <label for="von">Von:</label>
                    <span name="von" id="von"></span>
                </div>
                <div class="col-md-2">
                    <label for="bis">Bis:</label>
                    <span name="bis" id="bis"></span>
                </div>
                <div class="col-md-3">
                    <label for="character">Character</label>
                    <ul id="character"></ul>
                    <button id="btn_anmelden" title='Anmelden' onclick="anmelden();" style="display:none;">Anmelden</button>
                </div>
                
            </div>
        </div>
        <div class="modal-footer">
            <h3>Footer</h3>
        </div>
    </div>
    
</div>
